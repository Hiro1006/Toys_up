<div class="container">
  <h2 class="mt-5">
    <%= @end_user.last_name %><%= @end_user.first_name %>さんの会員詳細
  </h2>
  <div class="row">
    <div class="col-xs-8 mt-3">
      <table class="table table-borderless">
        <tbody>
          <tr>
            <td>会員ID</td>
            <td><%= @end_user.id %></td>
          </tr>
          <tr>
            <td>氏名</td>
            <td>
              <%= @end_user.last_name %><%= @end_user.first_name %>
            </td>
          </tr>
          <tr>
            <td>郵便番号</td>
            <td><%= @end_user.postal_code %></td>
          </tr>
          <tr>
            <td>住所</td>
            <td><%= @end_user.address %></td>
          </tr>
          <tr>
            <td>メールアドレス</td>
            <td><%= @end_user.email%></td>
          </tr>
          <tr>
            <td>会員ステータス</td>
            <td>
              <% if @end_user.is_deleted == true %>
                <span class="text-danger">退会済</span>
              <% elsif @end_user.is_deleted == false %>
                <span class="text-success">有効会員</span>
              <% end %>
            </td>
          </tr>
          <tr>
            <td></td>
            <td>
              <div class="actions">
                <%= link_to "編集する",edit_admin_end_user_path(@end_user), class: "btn btn-success #{@end_user.id}" %>
              </div>
            </td>
            <td>
              <div class="actions">
              </div>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>