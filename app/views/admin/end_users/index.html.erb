<div class="container">
  <h2 class="mt-5">会員一覧</h2>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
      </tr>
    </thead>
    <% @end_users.each do |end_user| %>
    <tbody>
      <tr>
        <td>
          <i class="fas fa-search"></i>
          <%= end_user.id %>
        </td>
        <td>
          <i class="far fa-user"></i>
          <%= link_to admin_end_user_path(end_user), class: "btn-link" do %>
            <%= end_user.last_name %><%= end_user.first_name %>
          <% end %>
        </td>
        <td>
          <i class="fas fa-envelope"></i>
          <%= end_user.email %>
        </td>
        <td>
          <% if end_user.is_deleted == true %>
            <p>
              <span class="text-danger">退会済</span>
            </p>
          <% else %>
            <p>
              <span class="text-success">有効</span>
            </p>
          <% end %>
        </td>
      </tr>
    </tbody>
    <% end %>
  </table>
  <%= paginate @end_users %>
</div>